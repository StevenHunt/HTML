  <form action="MAILTO:Steven.c.hunt@gmail.com" method="post" enctype="text/plain">
  Name: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <input type="text" name="name" value="First and last name"> <br>
  Email Address:       <input type="text" name="mail" value="Your email address"> <br>
  Comment: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="text" name="comment" value="Your comment" size="80"> <br>
  <br>
  <input type="submit" value="Send">
  <input type="reset" value="Reset">
  </form>
  
