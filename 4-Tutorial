function myMap() {
var mapOptions = {
    center: new google.maps.LatLng(47.774948, -122.265953),
    zoom:10,

    // mapTypeId: google.maps.MapTypeId.HYBRID (Uncomment to make the map Hybrid)
}
var map = new google.maps.Map(document.getElementById("googleMap"), mapOptions);
}
</script>

